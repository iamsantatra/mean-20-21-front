{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "assignment-app": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/assignment-app",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "src/assets/css/vendors/icofont.css",
              "src/assets/css/vendors/themify.css",
              "src/assets/css/vendors/bootstrap.css",
              "src/assets/css/style.css",
              "src/assets/css/color-1.css",
              "src/assets/css/responsive.css"
            ],
            "scripts": [
              "src/assets/js/jquery-3.6.0.min.js",
              "src/assets/js/bootstrap/bootstrap.bundle.min.js",
              "src/assets/js/config.js",
              "src/assets/js/typeahead/handlebars.js",
              "src/assets/js/typeahead/typeahead.bundle.js",
              "src/assets/js/typeahead/typeahead.custom.js",
              "src/assets/js/typeahead-search/handlebars.js",
              "src/assets/js/script.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "assignment-app:build:production"
            },
            "development": {
              "browserTarget": "assignment-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "assignment-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "src/assets/css/vendors/font-awesome.css",
              "src/assets/css/vendors/icofont.css",
              "src/assets/css/vendors/themify.css",
              "src/assets/css/vendors/flag-icon.css",
              "src/assets/css/vendors/feather-icon.css",
              "src/assets/css/vendors/scrollbar.css",
              "src/assets/css/vendors/bootstrap.css",
              "src/assets/css/style.css",
              "src/assets/css/color-1.css",
              "src/assets/css/responsive.css"
            ],
            "scripts": [
              "src/assets/js/jquery-3.6.0.min.js",
              "src/assets/js/bootstrap/bootstrap.bundle.min.js",
              "src/assets/js/icons/feather-icon/feather.min.js",
              "src/assets/js/icons/feather-icon/feather-icon.js",
              "src/assets/js/scrollbar/simplebar.js",
              "src/assets/js/scrollbar/custom.js",
              "src/assets/js/config.js",
              "src/assets/js/jkanban/custom.js",
              "src/assets/js/typeahead/handlebars.js",
              "src/assets/js/typeahead/typeahead.bundle.js",
              "src/assets/js/typeahead/typeahead.custom.js",
              "src/assets/js/dragable/sortable.js",
              "src/assets/js/dragable/sortable-custom.js",
              "src/assets/js/typeahead-search/handlebars.js",
              "src/assets/js/script.js"
            ]
          }
        }
      }
    }
  }
}
